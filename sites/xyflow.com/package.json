    "next": "catalog:",
    "next-sitemap": "catalog:",
    "nextra": "catalog:",
    "nextra-theme-docs": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "xy-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "eslint-config-xyflow": "workspace:*",
    "postcss-lightningcss": "^1.0.1",
