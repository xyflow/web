{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_\\*",
    "NPM_XYFLOW_REACT",
    "SVELTE_FLOW_VERSION",
    "REACT_FLOW_VERSION",
    "VERCEL_ENV",
    "VERCEL_GIT_COMMIT_REF",
    "NOTION_API_SECRET",
    "BLOB_READ_WRITE_TOKEN",
    "GH_APP_ID",
    "GH_APP_INSTALL_ID",
    "GITHUB_APP_CLIENT_ID",
    "GITHUB_APP_CLIENT_SECRET",
    "GH_APP_PRIVATE_KEY",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "APP_URL"
  ],
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "lint"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".svelte-kit/**",
        ".vercel/**",
        "public/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ]
    },
    "reactflow-website#build": {
      "dependsOn": ["^build", "^ui-components#generate-registry", "^example-apps#build", "lint"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env",
        "VERCEL_ENV",
        "VERCEL_GIT_COMMIT_REF"
      ]
    },
    "svelteflow-website#build": {
      "dependsOn": ["^build", "^example-apps#build", "lint"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ]
    },
    "xyflow-website#build": {
      "dependsOn": ["^build", "^example-apps#build", "lint"],
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ]
    },
    "ui-components#generate-registry": {
      "outputs": ["public/**"],
      "inputs": ["$TURBO_DEFAULT$"]
    },
    "lint": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env",
        "VERCEL_ENV",
        "VERCEL_GIT_COMMIT_REF"
      ]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$", ".env.test.local", ".env.test", ".env"]
    },
    "check-types": {
      "inputs": ["$TURBO_DEFAULT$"]
    }
  }
}
