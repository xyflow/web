{
  "name": "svelte-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "./create_endpoints.sh && vite dev",
    "build": "./create_endpoints.sh && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "create:endpoints": "./create_endpoints.sh"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.1.1",
    "@sveltejs/kit": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "postcss": "^8.4.36",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "type": "module",
  "dependencies": {
    "@dagrejs/dagre": "^1.1.1",
    "@threlte/core": "^6.1.0",
    "@types/dagre": "^0.7.51",
    "@types/three": "^0.155.1",
    "@xyflow/svelte": "0.1.1",
    "elkjs": "^0.9.2",
    "html-to-image": "^1.11.11",
    "svelte-feather-icons": "^4.0.1",
    "three": "^0.158.0",
    "xy-tsconfig": "workspace:*"
  }
}
