{
  "private": true,
  "scripts": {
    "vercel-submodule": "sh ./scripts/vercel-private-submodule.sh",
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter xyflow-docs",
    "dev:platform": "turbo run dev --filter xyflow-platform",
    "dev:style": "turbo run dev --filter xy-styleguide",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo node_modules"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.3",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3",
    "xy-eslint-config": "workspace:*"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "docs"
}
